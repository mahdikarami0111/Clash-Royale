<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="328.0" prefWidth="242.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SignupCon">
    <children>
        <AnchorPane prefHeight="304.0" prefWidth="242.0">
            <children>
                <TextField fx:id="userName" layoutX="12.0" layoutY="14.0" prefHeight="56.0" prefWidth="219.0" promptText="user name" />
                <PasswordField fx:id="password" layoutX="11.0" layoutY="87.0" prefHeight="56.0" prefWidth="219.0" promptText="password" />
                <PasswordField fx:id="confirmPassword" layoutX="11.0" layoutY="164.0" prefHeight="56.0" prefWidth="219.0" promptText="confirm password" />
                <Button layoutX="90.0" layoutY="228.0" mnemonicParsing="false" onAction="#signUp" text="sign up" />
                <Hyperlink layoutX="99.0" layoutY="267.0" onAction="#logIn" prefHeight="26.0" prefWidth="49.0" text="log in" />
            </children>
        </AnchorPane>
    </children>
</VBox>
