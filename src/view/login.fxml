<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="346.0" prefWidth="292.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginCon">
    <children>
        <AnchorPane prefHeight="348.0" prefWidth="292.0">
            <children>
                <TextField fx:id="userName" layoutX="28.0" layoutY="14.0" prefHeight="62.0" prefWidth="236.0" promptText="user name" />
                <PasswordField fx:id="passWord" layoutX="28.0" layoutY="111.0" prefHeight="62.0" prefWidth="236.0" promptText="password" />
                <Button fx:id="loginButton" layoutX="87.0" layoutY="211.0" mnemonicParsing="false" onAction="#login" prefHeight="24.0" prefWidth="124.0" text="log in" />
                <Hyperlink fx:id="signUpButton" layoutX="122.0" layoutY="266.0" onAction="#signUp" text="sign up" />
            </children>
        </AnchorPane>
    </children>
</VBox>